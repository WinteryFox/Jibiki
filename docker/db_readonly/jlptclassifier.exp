set timeout -1
 
spawn java -jar ./JibikiJLPTClassifier-1.0-SNAPSHOT-jar-with-dependencies.jar
 
expect "Enter the IP of your server\r"
 
send -- "localhost\r"
 
expect "Enter the target database name\r"
 
send -- "jibiki\r"
 
expect "Enter the username\r"
 
send -- "postgres\r"

expect "Enter the password\r"
 
send -- "\r"

